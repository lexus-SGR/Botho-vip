module.exports = {
  name: "unmuteall",
  description: "Unmute group chat ðŸ”Š",
  usage: "unmuteall",
  category: "security",
  react: "ðŸ”Š",
  sudo: true,
  async execute(sock, msg, args, from, sender, isGroup) {
    if (!isGroup) return sock.sendMessage(from, { text: "ðŸ”“ Group only command." }, { quoted: msg });
    await sock.groupSettingUpdate(from, "not_announcement");
    await sock.sendMessage(from, { text: "ðŸ”Š Group has been unmuted." }, { quoted: msg });
  }
};
